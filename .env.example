# Example environment variables for deployment
# Copy to `.env` and fill in real secrets before running the container

# Database
DB_HOST=postgres
DB_PORT=5432
DB_NAME=stock_db
DB_USER=stock_user
DB_PASS=stock_pass

# Provider API keys
# Multiple comma-separated Marketstack API keys are supported for rotation
MARKETSTACK_API_KEYS=your_key_1,your_key_2
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key

# Optional: Dagster
DAGSTER_HOME=/usr/src/app/dagster_home
